@model IEnumerable<ARSAN_Web.Models.MiembroJunta>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FechaInicio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaFin)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.JuntaDirectiva)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Propietario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cargo)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FechaInicio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaFin)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.JuntaDirectiva.IdJuntaDirectiva)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Propietario.Dpi)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cargo.Nombre)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdMiembroJunta">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdMiembroJunta">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdMiembroJunta">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
